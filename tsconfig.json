{
  "compilerOptions": {
    "baseUrl": "./front",
    "module": "es2015",
    "moduleResolution": "node",
    "allowSyntheticDefaultImports": true,
    "target": "ES5",
    "experimentalDecorators": true,
    "noImplicitAny": true,
    "noUnusedLocals": true,
    "sourceMap": false,
    "inlineSourceMap": true,
    "inlineSources": true,
    "pretty": true,
    "importHelpers": true,
    "jsx": "react",
    "lib": [
      "dom",
      "es5",
      "es2015",
      "es2015.promise",
      "es2015.iterable"
    ],
    "typeRoots": ["../node_modules/@types"]
  },
  "exclude": [
    "../node_modules"
  ]
}
